Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\Directory\Background\shell\powershell_admin]
@="Open Admin PowerShell here"
"HasLUAShield"=""

[HKEY_CLASSES_ROOT\Directory\Background\shell\powershell_admin\command]
@="powershell.exe -NoExit -Command \"Start-Process PowerShell -Verb RunAs -ArgumentList '-NoExit','-Command','Set-Location -LiteralPath ''%V'''\""
